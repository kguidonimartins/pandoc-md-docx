doi = {10.3170/2007-8-18312},
file = {:home/kguidonimartins/Documents/Mendeley Desktop/Dray, Sa{\"{i}}d, D{\'{e}}bias - 2008 - Spatial ordination of vegetation data using a generalization of Wartenberg's multivariate spatial correlat.pdf:pdf},
