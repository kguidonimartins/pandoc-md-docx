issn = {08888892},
journal = {Conservation Biology},
keywords = {Comparative method,Geographic range size,Growth form,Plant traits,Tropical flora,Vegetation types,Vulnerability,[phylogeny]ecophylogenetic,[phylogeny]phylogenetic{\_}signal,phylocom,rares{\_}dirty{\_}search},
mendeley-tags = {[phylogeny]ecophylogenetic,[phylogeny]phylogenetic{\_}signal,phylocom,rares{\_}dirty{\_}search},
