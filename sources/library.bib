isbn = {9788578110796},
issn = {1939-9170},
journal = {Ecology},
keywords = {community assembly,disturbance,functional,functional diversity,multidimensional trait space,multivariate analysis,ordination,theory,trait‐based ecology},
mendeley-tags = {functional,theory},
