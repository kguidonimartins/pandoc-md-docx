@article{CaldasdeCastro2006,
abstract = {Assessing the significance of multiple and dependent comparisons is$\backslash$nan important, and often ignored, issue that becomes more critical$\backslash$nas the size of data sets increases. If not accounted for, false-positive$\backslash$ndifferences are very likely to be identified. The need to address$\backslash$nthis issue has led to the development of a myriad of procedures to$\backslash$naccount for multiple testing. The simplest and most widely used technique$\backslash$nis the Bonferroni method, which controls the probability that a true$\backslash$nnull hypothesis is incorrectly rejected. However, it is a very conservative$\backslash$nprocedure. As a result, the larger the data set the greater the chances$\backslash$nthat truly significant differences will be missed. In 1995, a new$\backslash$ncriterion, the false discovery rate (FDR), was proposed to control$\backslash$nthe proportion of false declarations of significance among those$\backslash$nindividual deviations from null hypotheses considered to be significant.$\backslash$nIt is more powerful than all previously proposed methods. Multiple$\backslash$nand dependent comparisons are also fundamental in spatial analysis.$\backslash$nAs the number of locations increases, assessing the significance$\backslash$nof local statistics of spatial association becomes a complex matter.$\backslash$nIn this article we use empirical and simulated data to evaluate the$\backslash$nuse of the FDR approach in appraising the occurrence of clusters$\backslash$ndetected by local indicators of spatial association. Results show$\backslash$na significant gain in identification of meaningful clusters when$\backslash$ncontrolling the FDR, in comparison to more conservative approaches.$\backslash$nWhen no control is adopted, false clusters are likely to be identified.$\backslash$nIf a conservative approach is used, clusters are only partially identified$\backslash$nand true clusters are largely missed. In contrast, when the FDR approach$\backslash$nis adopted, clusters are fully identified. Incorporating a correction$\backslash$nfor spatial dependence to conservative methods improves the results,$\backslash$nbut not enough to match those obtained by the FDR approach.},
author = {{Caldas de Castro}, Marcia and Singer, Burton H.},
doi = {10.1111/j.0016-7363.2006.00682.x},
file = {:home/kguidonimartins/Documents/Mendeley Desktop/Caldas de Castro, Singer - 2006 - Controlling the false discovery rate A new application to account for multiple and dependent tests in.pdf:pdf},
isbn = {1538-4632},
issn = {00167363},
journal = {Geographical Analysis},
keywords = {false{\_}discovery{\_}rate},
mendeley-tags = {false{\_}discovery{\_}rate},
number = {2},
pages = {180--208},
title = {{Controlling the false discovery rate: A new application to account for multiple and dependent tests in local statistics of spatial association}},
volume = {38},
year = {2006}
}
