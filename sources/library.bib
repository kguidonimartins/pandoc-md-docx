keywords = {Biodiversity,Conservation of Natural Resources,Ecology,Likelihood Functions,Stochastic Processes,heterogeinity-richness{\_}relationship,rares{\_}dirty{\_}search},
mendeley-tags = {heterogeinity-richness{\_}relationship,rares{\_}dirty{\_}search},
