month = {nov},
number = {11},
pages = {2830--8},
pmid = {18051652},
title = {{A multivariate analysis of beta diversity across organisms and environments.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18051652},
volume = {88},
