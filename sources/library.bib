month = {jul},
number = {6192},
pages = {1236828},
pmid = {24994657},
title = {{Human evolution. Evolution of early Homo: an integrated biological perspective.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24994657},
volume = {345},
year = {2014}
}
