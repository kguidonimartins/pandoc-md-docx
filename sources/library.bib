isbn = {1461-0248},
issn = {1461023X},
journal = {Ecology Letters},
keywords = {Community assembly,Competition,Environmental filtering,Functional similarity,Niche similarity,Phylogenetic relatedness,Plant interaction,Traits hierarchy,[phylogeny]ecophylogenetic,bayes,community assembly,trait-based},
