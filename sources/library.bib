isbn = {0012-9658},
issn = {00129658},
journal = {Ecology},
keywords = {Age,Costa Rica,Crown illumination index,Environmental stochasticity,Integral projection model,La selva,Light,Markov chains,Mortality trajectory,Remaining life expectancy,Size structure,Tropical trees,rares{\_}dirty{\_}search},
mendeley-tags = {rares{\_}dirty{\_}search},
number = {10},
pages = {2766--2778},
