journal = {Ecography},
keywords = {MaxEnt,rares{\_}dirty{\_}search},
mendeley-tags = {MaxEnt,rares{\_}dirty{\_}search},
number = {10},
pages = {1058--1069},
title = {{A practical guide to MaxEnt for modeling species' distributions: What it does, and why inputs and settings matter}},
volume = {36},
