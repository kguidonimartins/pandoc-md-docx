@article{Hardy2012,
abstract = {Aim $\backslash$nWe addressed the roles of environmental ?ltering, historical biogeography and evolutionary niche conservatism on the phylogenetic structure of tropical tree communities with the following questions. (1) What is the impact of mesoclimatic gradients and dispersal limitation on phylogenetic turnover and species turnover? (2) How does phylogenetic turnover between continents compare in intensity with the turnover driven by climatic gradients at a regional scale? (3) Are independent phylogenetic reconstructions of the mesoclimatic niche of clades congruent between continents? $\backslash$n$\backslash$nLocation $\backslash$nPanama Canal Watershed and Western Ghats (India), two anciently divergent biogeographic contexts but with comparable rainfall gradients. $\backslash$n$\backslash$nMethods $\backslash$nUsing ?oristic data for 50 1-ha plots in each region, independent mea- sures of phylogenetic turnover (P ST ) and species turnover (Jaccard) between plots were regressed on geographic and ecological distances. Mesoclimatic niches were reconstructed for each node of the phylogeny and compared between the two continents. $\backslash$n$\backslash$nResults $\backslash$n(1) The phylogenetic turnover within each region is best explained by mesoclimatic differences (environmental ?ltering), while species turnover depends both on mesoclimatic differences and geographic distances (dispersal limitation). (2) The phylogenetic turnover between continents (P ST = 0.009) is comparable to that caused by mesoclimatic gradients within regions (P ST = 0.010) and both effects seem cumulative. (3) Independent phylogenetic reconstructions of the mesocli- matic niches were strongly correlated between the two continents (r = 0.61), despite the absence of shared species. $\backslash$n$\backslash$nMain conclusions $\backslash$nOur results demonstrate a world-wide deep phylogenetic signal for mesoclimatic niche within a biome, indicating that positive phylogenetic turnover at a regional scale re?ects environmental ?ltering in plant communities},
author = {Hardy, Olivier J. and Couteron, Pierre and Munoz, Fran{\c{c}}ois and Ramesh, B. R. and P{\'{e}}lissier, Rapha{\"{e}}l},
doi = {10.1111/j.1466-8238.2011.00742.x},
file = {:home/kguidonimartins/Documents/Mendeley Desktop/Hardy et al. - 2012 - Phylogenetic turnover in tropical tree communities Impact of environmental filtering, biogeography and mesoclimati.pdf:pdf},
isbn = {1466-822X},
issn = {1466822X},
journal = {Global Ecology and Biogeography},
keywords = {Assembly processes,Community phylogenetics,Environmental filtering,Neutral communities,Phylodiversity,Phylogenetic signal,Phylogenetic turnover,Species turnover,Tropical rain forest,[phylogeny]ecophylogenetic,phylo-metrics},
mendeley-tags = {[phylogeny]ecophylogenetic,phylo-metrics},
number = {10},
pages = {1007--1016},
