journal = {Journal of Vegetation Science},
keywords = {Common garden experiment,Comparative ecology,Field observations,Functional trait,Inter-specific variability,Intra-specific variability,Species ranking,Trait database,intraspecific{\_}variation},
mendeley-tags = {intraspecific{\_}variation},
