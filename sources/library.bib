@article{Calippo2008,
abstract = {Em meio {\'{a}} Plan{\'{i}}cie Costeira de Canan{\'{e}}ia-lguape (litoral sul do estado de S{\~{a}}o Paulo, Brasil) foram encontradas evid{\^{e}}ncias de uma presen{\c{c}}a sambaquieira que remonta a pelo menos 8000 anos AP e que pode ter parte de seus s{\'{i}}tios associados a uma plataforma continental hoje submersa. A fim de contextualiz{\'{a}}-los ao longo das modifica{\c{c}}{\~{o}}es ambientais holoc{\^{e}}nicas, esses s{\'{i}}tios e a ocupa{\c{c}}{\~{a}}o sambaquieira que se estabeleceu nessa regi{\~{a}}o foram pesquisados a partir de abordagens focadas na Arqueologia subaqu{\'{a}}tica, na Geoarqueol{\'{o}}gica (Waters 1992) e no estudo dos processos de forma{\c{c}}{\~{a}}o adotados por Stein (1992). Al{\'{e}}m de identificar que o potencial de preserva{\c{c}}{\~{a}}o e a distribui{\c{c}}{\~{a}}o dos sambaquis dessa regi{\~{a}}o apresentam uma forte correla{\c{c}}{\~{a}}o com as varia{\c{c}}{\~{o}}es do n{\'{i}}vel relativo do mar, algumas das evid{\^{e}}ncias sugerem tamb{\'{e}}m que esses sambaquis s{\~{a}}o estruturas intencionalmente constru{\'{i}}das (imbu{\'{i}}das de significado e monumentalidade), cujos representantes mais antigos (sambaquis submersos) podem ter se preservado tanto em {\'{a}}reas hoje emersas como submersas. Contribuindo, assim, tamb{\'{e}}m para o desenvolvimento da Arqueologia subaqu{\'{a}}tica brasileira e para o estudo das varia{\c{c}}{\~{o}}es do n{\'{i}}vel relativo do mar. En la planicie costera de Canan{\'{e}}ia-lguape (extremo sur de la costa del Estado de S{\~{a}}o Paulo), se ha descubierto evidencia de los constructores de conchales que data de hace aproximadamente 8000 a{\~{n}}os atr{\'{a}}s y que puede estar asociada con una regi{\'{o}}n que ha estado sumergida. Para comprender la aparici{\'{o}}n de estos conchales y su contexto en relaci{\'{o}}n con las variaciones del nivel del mar en el Holoceno, ellos fueron estudiados a trav{\'{e}}s de m{\'{e}}todos y t{\'{e}}cnicas de la Arqueolog{\'{i}}a Subacu{\'{a}}tica, utilizando las interpretaciones geoarqueol{\'{o}}gicas de Waters (1992) y los estudios de Procesos de Formaci{\'{o}}n de Stein (1992). Por lo tanto, el potencial para la preservaci{\'{o}}n de conchales y su distribuci{\'{o}}n en esta {\'{a}}rea muestra una fuerte correlaci{\'{o}}n con los cambios en el nivel del mar que se produjeron despu{\'{e}}s de la {\'{u}}ltima regresi{\'{o}}n m{\'{a}}xima del Holoceno. Esta evidencia tambi{\'{e}}n indica que estos sitios son estructuras construidas intencionalmente (impregnadas de "monumentalidad" y significado), y los m{\'{a}}s antiguos pueden ser conservados bajo o encima del actual nivel del mar. De esta manera, este trabajo contribuye al desarrollo de la Arqueolog{\'{i}}a Subacu{\'{a}}tica de Brasil y a los estudios de los cambios del nivel del mar. In the Coastal Plain of Canan{\'{e}}ia-lguape (extreme south of the State of S{\~{a}}o Paulo's coastline), evidence {\ldots}},
author = {Calippo, Fl{\'{a}}vio.},
journal = {Revista de Arqueolog{\'{i}}a Americana},
pages = {153--171},
title = {{Os Sambaquis submersos do Baixo Vale do Ribeira: um estudo de caso de Arqueologia Subaqu{\'{a}}tica}},
volume = {26},
year = {2008}
}
