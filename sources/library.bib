journal = {Ecology Letters},
keywords = {Coexistence,Ecological drift,Extinction risk,Neutral theory,Niche,Nonlinear averaging,Stability,Stochasticity,coexistence,rares{\_}dirty{\_}search},
mendeley-tags = {coexistence,rares{\_}dirty{\_}search},
