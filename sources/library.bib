issn = {1461-0248},
journal = {Ecology letters},
keywords = {Animals,Biodiversity,Ecology,Models,SADs,Theoretical,raras e comuns,rares{\_}dirty{\_}search,theory},
mendeley-tags = {SADs,raras e comuns,rares{\_}dirty{\_}search,theory},
month = {oct},
