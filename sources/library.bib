journal = {Biology and Philosophy},
keywords = {Accuracy,Ecology,Heuristic,Idealization,Mathematics,Model,Pragmatism,Prediction,Theory},
number = {2-3},
pages = {231--255},
title = {{Idealized, inaccurate but successful: A pragmatic approach to evaluating models in theoretical ecology}},
volume = {20},
year = {2005}
}
