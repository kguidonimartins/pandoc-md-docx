doi = {10.1890/09-0385.1},
file = {:home/kguidonimartins/Documents/Mendeley Desktop/Ravent{\'{o}}s et al. - 2010 - Evidence for the spatial segregation hypothesis a test with nine-year survivorship data in a Mediterranean sh.pdf:pdf},
journal = {Ecology},
keywords = {bolker and pacala 1999,chesson 2000,facilitation,fire,hypothesis,interactions,kareiva 1997,mediterranean,scramble and contest competition,spatial point pattern analysis},
mendeley-tags = {interactions},
number = {7},
