file = {:home/kguidonimartins/Documents/Mendeley Desktop/Tripathi et al. - 2014 - Spatial Scaling Effects on Soil Bacterial Communities in Malaysian Tropical Forests - Microbial Ecology.pdf:pdf},
issn = {00953628},
journal = {Microbial Ecology},
keywords = {multi-scale ordination,spatial-explicity},
mendeley-tags = {multi-scale ordination,spatial-explicity},
pages = {247--258},
