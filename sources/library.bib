@book{Stehmann2009,
abstract = {Os resultados de estudos, pesquisas e levantamentos t{\'{e}}cnico-cient{\'{i}}ficos realizados sobre a Mata Atl{\^{a}}ntica, especialmente nas {\'{u}}ltimas duas d{\'{e}}cadas, confirmam a not{\'{a}}vel diversidade de esp{\'{e}}cies desse Dom{\'{i}}nio. As estrat{\'{e}}gias de conserva{\c{c}}{\~{a}}o na regi{\~{a}}o sempre foram subsidiadas por essas informa{\c{c}}{\~{o}}es, mas com {\^{e}}nfase na fauna, principalmente dos primatas considerados como “esp{\'{e}}cies-bandeira”. Entretanto, a complexidade e a exuber{\^{a}}ncia da Mata Atl{\^{a}}ntica exige um novo olhar, capaz de desvendar outros segredos e verdadeiras j{\'{o}}ias desse patrim{\^{o}}nio biol{\'{o}}gico, ou seja, a sua flora. Esta publica{\c{c}}{\~{a}}o {\'{e}} uma avalia{\c{c}}{\~{a}}o in{\'{e}}dita e valiosa sobre as plantas da Mata Atl{\^{a}}ntica. As informa{\c{c}}{\~{o}}es aqui apresentadas s{\~{a}}o fruto da participa{\c{c}}{\~{a}}o e da colabora{\c{c}}{\~{a}}o de dezenas de pesquisadores, evidenciando a grande capacidade institucional e a rede de parcerias atuante na regi{\~{a}}o. Os resultados confirmam a posi{\c{c}}{\~{a}}o de destaque da Mata Atl{\^{a}}ntica entre os mundiais de biodiversidade, assim como ressalta a necessidade de mais investimentos em ci{\^{e}}ncia hotspots e tecnologia, devido ao n{\'{u}}mero significativo de esp{\'{e}}cies novas revelado nesse bioma anualmente e as lacunas de conhecimento ainda existentes para alguns grupos biol{\'{o}}gicos. {\'{E}} importante enfatizar ainda que esta publica{\c{c}}{\~{a}}o torna acess{\'{i}}vel as informa{\c{c}}{\~{o}}es sobre a flora do Dom{\'{i}}nio Atl{\^{a}}ntico. Trata-se, portanto, de uma obra para todos envolvidos com pesquisa, conserva{\c{c}}{\~{a}}o e desenvolvimento sustent{\'{a}}vel. S{\~{a}}o dados essenciais para fornecer a liga{\c{c}}{\~{a}}o entre a an{\'{a}}lise cient{\'{i}}fica e a tomada de decis{\~{o}}es, t{\~{a}}o necess{\'{a}}rias para expandir os esfor{\c{c}}os de conserva{\c{c}}{\~{a}}o e a sustentabilidade desse que {\'{e}} um dos maiores reposit{\'{o}}rios de biodiversidade do planeta.},
author = {Stehmann, J.S. and Forzza, R.C. and Salino, A. and Sobral, M. and Costa, D.P. and Kamino, L.H.Y.},
isbn = {9788560035052},
keywords = {Brasil,Flora,Mata atl{\^{a}}ntica},
pages = {505},
title = {{Plantas da Floresta Atl{\^{a}}ntica.}},
url = {http://www.jbrj.gov.br/publica/livros{\_}pdf/plantas{\_}floresta{\_}atlantica.pdf},
year = {2009}
}
