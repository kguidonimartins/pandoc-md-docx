month = {sep},
number = {1},
pages = {070908024102002--???},
pmid = {478},
title = {{Facilitation in plant communities: the past, the present, and the future}},
url = {http://doi.wiley.com/10.1111/j.1365-2745.2007.01295.x},
volume = {96},
