journal = {Journal of Animal Ecology},
keywords = {community classification,ecological community,local richness,rares{\_}dirty{\_}search,regional richness,regional-to-local,saturation,species richness},
mendeley-tags = {rares{\_}dirty{\_}search,regional-to-local},
number = {1},
