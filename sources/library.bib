isbn = {0012-9658},
issn = {00129658},
journal = {Ecology},
keywords = {Abundance,Geographical range,Macroecology,O-A relationship,Plants,Rain forest,Rank-abundance curve,SADs,Sclerophyll woodland,raras,raras e comuns,rares{\_}dirty{\_}search},
mendeley-tags = {O-A relationship,SADs,raras,raras e comuns,rares{\_}dirty{\_}search},
