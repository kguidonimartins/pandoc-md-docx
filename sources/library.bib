journal = {The American naturalist},
keywords = {Ecology,Ecosystem,Models, Biological,Phylogeny,[phylogeny]ecophylogenetic,[phylogeny]phylogenetic{\_}signal,functional,phylocom,rares{\_}dirty{\_}search},
