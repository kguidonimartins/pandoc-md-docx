@article{Pelletier2009a,
abstract = {Abstract In two articles, we present ‘coregionalization analysis with a drift' (CRAD), a method to assess the multi-scale variability$\backslash$r$\backslash$nof and relationships between ecological variables from a multivariate spatial data set. In phase I of CRAD (the first article),$\backslash$r$\backslash$na deterministic drift component representing the large-scale pattern and a random component modeled as a second-order stationary$\backslash$r$\backslash$nprocess are estimated for each variable separately. In phase II (this article), a linear model of coregionalization (LMC)$\backslash$r$\backslash$nis fitted by estimated generalized least squares to the direct and cross experimental variograms of residuals (i.e., after$\backslash$r$\backslash$nthe removal of estimated drifts). Structural correlations and coefficients of determination at smaller scales are then computed$\backslash$r$\backslash$nfrom the estimated coregionalization matrices, while the estimated drifts are used to calculate pseudo coefficients at large$\backslash$r$\backslash$nscale. The performance of five procedures in estimating correlations and coefficients of determination was compared using$\backslash$r$\backslash$na Monte Carlo study. In four CRAD procedures, drift estimation was based on local polynomials of order 0, 1, 2 (L0, L1, L2) or a global polynomial with forward selection of the basis functions; the fifth procedure was coregionalization analysis$\backslash$r$\backslash$n(CRA), in which large-scale patterns were modeled as a supplemental component in the LMC. In bivariate and multivariate analyses,$\backslash$r$\backslash$nthe uncertainty in the estimation of correlations and coefficients of determination could be related to the interference between$\backslash$r$\backslash$nspatial components within a bounded sampling domain. In the bivariate case, most procedures provided acceptable estimates$\backslash$r$\backslash$nof correlations. In regionalized redundancy analysis, uncertainty was highest for CRA, while L1 provided the best results overall. In a forest ecology example, the identification of scale-specific correlations between$\backslash$r$\backslash$nplant species diversity and soil and topographical variables illustrated the potential of CRAD to provide unique insight into$\backslash$r$\backslash$nthe functioning of complex ecosystems.},
author = {Pelletier, Bernard and Dutilleul, Pierre and Larocque, Guillaume and Fyles, James W.},
doi = {10.1007/s10651-008-0090-z},
file = {:home/kguidonimartins/Documents/Mendeley Desktop/Pelletier et al. - 2009 - Coregionalization analysis with a drift for multi-scale assessment of spatial relationships between ecologi(2).pdf:pdf},
isbn = {1065100800966},
issn = {13528505},
journal = {Environmental and Ecological Statistics},
keywords = {Bias in variogram of residuals,Bounded sampling domain,Estimated generalized least squares,Geostatistical models,Local versus global drift estimation,Small versus large scales of variability,coregionalization,multi-scale ordination},
mendeley-tags = {coregionalization,multi-scale ordination},
number = {4},
pages = {439--466},
title = {{Coregionalization analysis with a drift for multi-scale assessment of spatial relationships between ecological variables 1. Estimation of drift and random components}},
volume = {16},
year = {2009}
}
