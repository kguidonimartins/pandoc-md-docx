@article{Orrock2010,
abstract = {The outcome of competitive interactions is likely to be influenced by both competitive dominance (i.e. niche-based dynamics) and ecological drift (i.e. neutral dynamics governed by demographic stochasticity). However, spatial models of competition rarely consider the joint operation of these two processes. We develop a model based on the original competition-colonization trade-off model that incorporates niche and neutral processes and several realistic facets of ecological dynamics: it allows local competition (i.e. competition within a patch) to occur within communities of a finite size, it allows competitors to vary in the degree of competitive asymmetry, and it includes the role of local migration (i.e. propagule pressure). The model highlights the role of community size, i.e. the number of competitors in the local community, in mediating the relative importance of stochastic and deterministic forces. In metacommunities where local communities are small, ecological drift is substantial enough that strong competitors become effectively neutral, creating abrupt changes in the outcome of competition not predicted by the standard competition-colonization trade-off. Importantly, the model illustrates that, even when other aspects of species interactions (e.g. migration ability, competitive ability) are unchanged, local community size can alter the dynamics of metacommunity persistence. Our work demonstrates that activities which reduce the size of local communities, such as habitat destruction and degradation, effectively compound the extinction debt.},
author = {Orrock, John L and Watling, James I},
doi = {10.1098/rspb.2009.2344},
file = {:home/kguidonimartins/Documents/Mendeley Desktop/Orrock, Watling - 2010 - Local community size mediates ecological drift and competition in metacommunities. - Proceedings. Biological sc.pdf:pdf},
issn = {1471-2954},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Computer Simulation,Ecosystem,Models,Population Density,Population Dynamics,Theoretical,rares{\_}dirty{\_}search},
mendeley-tags = {rares{\_}dirty{\_}search},
month = {jul},
number = {1691},
pages = {2185--91},
pmid = {20236983},
title = {{Local community size mediates ecological drift and competition in metacommunities.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2880146{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {277},
year = {2010}
}
