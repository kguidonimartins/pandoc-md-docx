isbn = {9780226350646},
issn = {9780226350646},
journal = {Metacommunities spatial dynamics and ecological communities},
number = {January},
pages = {335--354},
title = {{Competing theories for competitive metacommunities}},
url = {http://books.google.com/books?hl=en{\&}lr={\&}id=J4iy6Jl0a0MC{\&}oi=fnd{\&}pg=PA335{\&}dq=Competing+theories+for+competitive+Metacommunities{\&}ots=zabjzh4y4a{\&}sig=SvU3MrLbZ5fZ-C4xp7omuJTBf9Q},
