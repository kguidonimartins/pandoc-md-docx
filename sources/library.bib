journal = {Trends in Ecology and Evolution},
keywords = {functional,theory},
mendeley-tags = {functional,theory},
number = {12},
pages = {766--779},
pmid = {26519235},
publisher = {Elsevier Ltd},
title = {{So Many Variables: Joint Modeling in Community Ecology}},
