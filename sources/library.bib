journal = {Ecology},
keywords = {Environmental heterogeneity,Florida,Ilex,Niche conservatism,Overdispersion,Phylogenetic structure of communities,Pinus,Quercus,Taxonomic scale,Trait convergence,[phylogeny]ecophylogenetic,[phylogeny]phylogenetic{\_}signal,ecophylo-scale,scale},
