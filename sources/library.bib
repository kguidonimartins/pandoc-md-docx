file = {:home/kguidonimartins/Documents/Mendeley Desktop/L{\"{o}}bel, Sn{\"{a}}ll, Rydin - 2006 - Species richness patterns and metapopulation processes - Evidence from epiphyte communities in boreo-nemo.pdf:pdf},
isbn = {0906-7590},
