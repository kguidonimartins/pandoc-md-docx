issn = {00030147},
journal = {The American Naturalist},
keywords = {community assembly,dispersal,interactions,limitation,niche differentiation,regional species pool,scale,species{\_}pool},
mendeley-tags = {community assembly,interactions,species{\_}pool},
number = {1},
pages = {000--000},
title = {{Process-Based Species Pools Reveal the Hidden Signature of Biotic Interactions Amid the Influence of Temperature Filtering}},
