journal = {Forest Ecology and Management},
keywords = {Biomass,Forest productivity,Forest regeneration,Forest succession,Land use history,Land use intensity,Neotropics,Pasture conversion,Tropical rainforest,Tropical soils,rares{\_}dirty{\_}search},
mendeley-tags = {rares{\_}dirty{\_}search},
