keywords = {Connectivity metrics,Conservation biology,Dispersal kernels,Ecological networks,Graph theory,Habitat connectivity,Habitat fragmentation,Landscape configuration,Population spread,Species dispersal,rares{\_}dirty{\_}search},
mendeley-tags = {rares{\_}dirty{\_}search},
number = {1},
pages = {171--182},
title = {{EDITOR'S CHOICE: Stepping stones are crucial for species' long-distance dispersal and range expansion through habitat networks}},
volume = {51},
