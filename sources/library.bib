journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {@ler,Biodiversity,Ecosystem,Environment,Population Dynamics,Soil,Soil: chemistry,Trees,Trees: physiology,Tropical Climate,local scale,solos,topografia},
