isbn = {20457758},
issn = {20457758},
journal = {Ecology and Evolution},
keywords = {R,[phylogeny]ecophylogenetic,[phylogeny]phylogenetic{\_}signal,autocorrelation,comparative analysis,phylogenetic,phylogenetic correlogram,r software,signal,trait evolution},
mendeley-tags = {R,[phylogeny]ecophylogenetic,[phylogeny]phylogenetic{\_}signal},
