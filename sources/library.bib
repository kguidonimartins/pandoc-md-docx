@article{Brooks2004,
abstract = {Protected areas are widely recognized for their value in the conservation of biodiversity. Their establishment and management sometimes impose costs, however, such as the direct cost of protection and management activities and the opportunity cost to local residents who have to forego natural resource extraction. It is therefore essential that the creation of protected areas be planned systematically to ensure that costs are minimized and benefits maximized. This entails setting targets for the overall set of features that should ideally be conserved through protected areas and defining parameters to assess current protected-area networks and guide their expansion (Margules {\&} Pressey 2000).  The Fifth World Parks Congress (WPC, September 2003, Durban, South Africa, http://iucn.org/themes/wcpa/wpc2003/) provided fertile ground for the discussion of targets for biodiversity conservation planning, particularly through Workshop Stream 7 on "Building Comprehensive Protected Area Systems." Here, we build on these discussions to argue that, despite a range of shortcomings, species data provide the best basis for setting these targets.},
author = {Brooks, Thomas M. and {Da Fonseca}, Gustavo a B and Rodrigues, Ana S L},
doi = {10.1111/j.1523-1739.2004.01836.x},
file = {:home/kguidonimartins/Documents/Mendeley Desktop/Brooks, Da Fonseca, Rodrigues - 2004 - Protected areas and species - Conservation Biology.pdf:pdf},
isbn = {1523-1739},
issn = {08888892},
journal = {Conservation Biology},
pages = {616--618},
pmid = {2757},
title = {{Protected areas and species}},
volume = {18},
year = {2004}
}
