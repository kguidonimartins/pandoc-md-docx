author = {Zak, Donald R. and Hairston, Anne and Grigal, David F.},
file = {:home/kguidonimartins/Documents/Mendeley Desktop/Zak, Hairston, Grigal - 1991 - Topographic influences on nitrogen cycling within an upland pin oak ecosystem - Forest Science.pdf:pdf},
isbn = {0015-749X},
issn = {0015749X},
journal = {Forest Science},
