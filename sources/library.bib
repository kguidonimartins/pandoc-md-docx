journal = {New Phytologist},
keywords = {Coastal forest,Ectomycorrhizal (ECM) fungi,Ectomycorrhizal formation,MultivariateSEM,Mycorrhizal communities,Nitrogen-fixing ree,Pinus thunbergii (Japanese black pine),Robinia pseudoacacia (black locust),SEM,Seedling},
mendeley-tags = {MultivariateSEM,SEM},
