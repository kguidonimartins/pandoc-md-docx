isbn = {1466-8238},
issn = {1466822X},
journal = {Global Ecology and Biogeography},
keywords = {SpeciesDistributionModels,absence,correspondence,ecological niche model,gurutzeta guillera-arroita,habitat model,imperfect detection,presence,presence-background,presence-only,prevalence,sampling bias},
mendeley-tags = {SpeciesDistributionModels},
pages = {n/a--n/a},
title = {{Is my species distribution model fit for purpose? Matching data and models to applications}},
