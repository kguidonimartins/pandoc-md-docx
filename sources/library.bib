@article{Odenbaugh2005,
abstract = {Ecologists attempt to understand the diversity of life with mathematical models. Often, $\backslash$nmathematical models contain simplifying idealizations designed to cope with the blooming, buzzing $\backslash$nconfusion of the natural world. This strategy frequently issues in models whose predictions are $\backslash$ninaccurate. Critics of theoretical ecology argue that only predictively accurate models are successful $\backslash$nand contribute to the applied work of conservation biologists. Hence, they think that much of the $\backslash$nmathematical work of ecologists is poor science. Against this view, I argue that model building is $\backslash$nsuccessful even when models are predictively inaccurate for at least three reasons: models allow $\backslash$nscientists to explore the possible behaviors of ecological systems; models give scientists simplified $\backslash$nmeans by which they can investigate more complex systems by determining how the more complex $\backslash$nsystem deviates from the simpler model; and models give scientists conceptual frameworks through $\backslash$nwhich they can conduct experiments and fieldwork. Critics often mistake the purposes of model $\backslash$nbuilding, and once we recognize this, we can see their complaints are unjustified. Even though $\backslash$nmodels in ecology are not always accurate in their assumptions and predictions, they still contribute $\backslash$nto successful science.},
author = {Odenbaugh, Jay},
doi = {10.1007/s10539-004-0478-6},
file = {:home/kguidonimartins/Documents/Mendeley Desktop/Odenbaugh - 2005 - Idealized, inaccurate but successful A pragmatic approach to evaluating models in theoretical ecology - Biology and P.pdf:pdf},
