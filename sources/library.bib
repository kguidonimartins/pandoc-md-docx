journal = {Journal of Ecology},
keywords = {Coexistence mechanisms,Community ecology,Determinants of plant community diversity and stru,Functional space,Niche limitation,Plants,Rooting patterns,Saltmarsh,[phylogeny]ecophylogenetic,[phylogeny]phylogenetic{\_}signal},
mendeley-tags = {[phylogeny]ecophylogenetic,[phylogeny]phylogenetic{\_}signal},
