journal = {Ecology letters},
keywords = {Biodiversity,Biological Evolution,Biomass,Conservation of Natural Resources,Ecosystem,Environment,Phylogeny,Plant Development,Plant Physiological Phenomena,Plants,Plants: classification,Population Dynamics,Population Growth,Species Specificity,[phylogeny]ecophylogenetic,[phylogeny]phylogenetic{\_}signal,phylocom},
mendeley-tags = {[phylogeny]ecophylogenetic,[phylogeny]phylogenetic{\_}signal,phylocom},
month = {nov},
number = {11},
