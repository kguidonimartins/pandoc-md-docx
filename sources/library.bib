keywords = {454 pyrosequencing,ITS2,MultivariateSEM,SEM,dispersal limitation,ectomycorrhizal (EM) fungal community,plant community,subtropical secondary forest succession},
mendeley-tags = {MultivariateSEM,SEM},
number = {2},
pages = {771--785},
