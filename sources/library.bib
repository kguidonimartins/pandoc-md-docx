issn = {1095-9203},
journal = {Science (New York, N.Y.)},
keywords = {Animals,Biodiversity,Biological,Birds,Ecosystem,Extinction,Fishes,Introduced Species,Invertebrates,Mammals,Plants,Population Dynamics,Time Factors},
