journal = {Ecology Letters},
keywords = {Community phylogenetics,Competitive ability,Functional trait,Interspecific competition,Niche conservatism,Niche difference,Phylogenetic dispersion,Phylogenetic relatedness,Species coexistence,Species extinction,[phylogeny]ecophylogenetic,phylocom},
mendeley-tags = {[phylogeny]ecophylogenetic,phylocom},
