keywords = {SpeciesDistributionModels},
language = {en},
mendeley-tags = {SpeciesDistributionModels},
month = {nov},
number = {11},
pages = {1084--1091},
title = {{Environmental filters reduce the effects of sampling bias and improve predictions of ecological niche models}},
url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1600-0587.2013.00441.x/abstract http://onlinelibrary.wiley.com/store/10.1111/j.1600-0587.2013.00441.x/asset/ecog441.pdf?v=1{\&}t=ihfcsvok{\&}s=845da4c1a514849ccc29a205bd4ff76901879754},
