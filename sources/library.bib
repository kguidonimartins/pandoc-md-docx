file = {:home/kguidonimartins/Documents/Mendeley Desktop/Steinbauer et al. - 2012 - Current measures for distance decay in similarity of species composition are influenced by study extent and g.pdf:pdf},
isbn = {1466-8238},
issn = {1466822X},
journal = {Global Ecology and Biogeography},
keywords = {Beta diversity,Biodiversity,Dissimilarity,Ecological gradient,Scale dependence,Simpson similarity,Species turnover,S{\o}rensen similarity,rares{\_}dirty{\_}search,scale,scale-dependence},
