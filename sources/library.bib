journal = {Journal of Ecology},
keywords = {Dispersal mode,Dispersal syndrome,Fecundity,Hierarchical Bayesian model,Primary seed dispersal,Seed limitation,Seed production,Seed rain,Seed shadow,Tropical tree,seeds},
