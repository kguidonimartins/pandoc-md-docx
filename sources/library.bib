journal = {Acta Limnologica Brasiliensia},
keywords = {a maioria dos estudos,cca,dissimilarity,e fazer,ec{\'{o}}logos geralmente estimam m{\'{e}}dias,em ecologia de comunidades,foca na diversidade local,hierarchical scale,mas dedicam pouca aten{\c{c}}{\~{a}}o,o estudo da varia{\c{c}}{\~{a}}o,para entender sistemas naturais,predi{\c{c}}{\~{o}}es,raras e comuns,rare species,resumo,temporal turnover,to{\_}larger{\_}scales?,varia{\c{c}}{\~{a}}o,{\`{a}},{\'{e}} um aspecto chave},
