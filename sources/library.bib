doi = {10.1016/S0304-3800(02)00196-5},
eprint = {S0304380002001965},
file = {:home/kguidonimartins/Documents/Mendeley Desktop/Miller, Franklin - 2002 - Modeling the distribution of four vegetation alliances using generalized linear models and classification tree.pdf:pdf},
isbn = {1619594803},
issn = {03043800},
journal = {Ecological Modelling},
keywords = {Classification tree,GLM,Generalized linear model,Indicator kriging,Mojave Desert,Predictive vegetation modeling.,aspect,aspectos da topografia,machine-learning,topografia},
mendeley-tags = {GLM,aspect,aspectos da topografia,machine-learning,topografia},
pages = {227--247},
pmid = {583},
title = {{Modeling the distribution of four vegetation alliances using generalized linear models and classification trees with spatial dependence}},
