journal = {Methods in Ecology and Evolution},
keywords = {Functional dispersion,Functional divergence,Functional evenness,Functional richness,Plant functional traits,Rao's entropy,Trait sampling,rares{\_}dirty{\_}search},
mendeley-tags = {rares{\_}dirty{\_}search},
number = {1},
